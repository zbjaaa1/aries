From 9a73e385c723a9481552af1875dcdc2b03838de2 Mon Sep 17 00:00:00 2001
From: Aster Boese <asterboese@mailbox.org>
Date: Mon, 24 Mar 2025 15:43:38 -0400
Subject: [PATCH] rust: vendor deps with git

---
 rust/Cargo.lock |  7 ++++++-
 rust/Cargo.toml | 10 +++++-----
 2 files changed, 11 insertions(+), 6 deletions(-)

diff --git a/rust/Cargo.lock b/rust/Cargo.lock
index ac41e15..3a03b31 100644
--- a/rust/Cargo.lock
+++ b/rust/Cargo.lock
@@ -1,18 +1,21 @@
 # This file is automatically @generated by Cargo.
 # It is not intended for manual editing.
-version = 3
+version = 4
 
 [[package]]
 name = "bitflags"
 version = "1.3.2"
+source = "git+https://github.com/rust-lang/bitflags.git?rev=ed185cfb1c447c1b4bd6ac021c9ec3bb02c9e2f2#ed185cfb1c447c1b4bd6ac021c9ec3bb02c9e2f2"
 
 [[package]]
 name = "cfg-if"
 version = "1.0.0"
+source = "git+https://github.com/rust-lang/cfg-if.git?rev=e60fa1efeab0ec6e90c50d93ec526e1410459c23#e60fa1efeab0ec6e90c50d93ec526e1410459c23"
 
 [[package]]
 name = "fatfs"
 version = "0.4.0"
+source = "git+https://github.com/rafalh/rust-fatfs.git?rev=87fc1ed5074a32b4e0344fcdde77359ef9e75432#87fc1ed5074a32b4e0344fcdde77359ef9e75432"
 dependencies = [
  "bitflags",
  "log",
@@ -21,6 +24,7 @@ dependencies = [
 [[package]]
 name = "log"
 version = "0.4.17"
+source = "git+https://github.com/rust-lang/log.git?rev=7fb28c36c7a418912612ab37ab49bd4ca1a3a7f5#7fb28c36c7a418912612ab37ab49bd4ca1a3a7f5"
 dependencies = [
  "cfg-if",
 ]
@@ -36,3 +40,4 @@ dependencies = [
 [[package]]
 name = "uuid"
 version = "1.7.0"
+source = "git+https://github.com/uuid-rs/uuid.git?rev=cefc353334784f91f3cab97fa16bf7a1a141b800#cefc353334784f91f3cab97fa16bf7a1a141b800"
diff --git a/rust/Cargo.toml b/rust/Cargo.toml
index 39229e9..a0954ce 100644
--- a/rust/Cargo.toml
+++ b/rust/Cargo.toml
@@ -11,11 +11,11 @@ crate-type = [ "staticlib" ]
 
 # See more keys and their definitions at https://doc.rust-lang.org/cargo/reference/manifest.html
 [dependencies]
-fatfs = { path = "vendor/rust-fatfs", default-features = false, features = ["lfn", "alloc"] }
+fatfs = { git = "https://github.com/rafalh/rust-fatfs.git", rev = "87fc1ed5074a32b4e0344fcdde77359ef9e75432", default-features = false, features = ["lfn", "alloc"] }
 uuid = { version = "1.7.0", default-features = false }
 
 [patch.crates-io]
-uuid = { path = "vendor/uuid" }
-log = { path = "vendor/log" }
-bitflags = { path = "vendor/bitflags" }
-cfg-if = { path = "vendor/cfg-if" }
+uuid = { git = "https://github.com/uuid-rs/uuid.git", rev = "cefc353334784f91f3cab97fa16bf7a1a141b800" }
+log = { git = "https://github.com/rust-lang/log.git", rev = "7fb28c36c7a418912612ab37ab49bd4ca1a3a7f5" }
+bitflags = { git = "https://github.com/rust-lang/bitflags.git", rev = "ed185cfb1c447c1b4bd6ac021c9ec3bb02c9e2f2" }
+cfg-if = { git = "https://github.com/rust-lang/cfg-if.git", rev = "e60fa1efeab0ec6e90c50d93ec526e1410459c23" }
-- 
2.49.0

